#!/bin/bash

# Launch or focus on a given TUI identified by the passed in as the command.
# Use by commands like barchti-launch-wifi to ensure there is only one wifi configuration screen open.

APP_ID="org.barchti.$(basename "$1")"
LAUNCH_COMMAND="barchti-launch-tui $@"

exec barchti-launch-or-focus "$APP_ID" "$LAUNCH_COMMAND"
